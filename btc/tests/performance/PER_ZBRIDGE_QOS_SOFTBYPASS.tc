#!/usr/bin/expect --

proc PER_ZBRIDGE_QOS_SOFTBYPASS {} {
#--------------------------------------------------------------------------------------------#
variable p [namespace tail [lindex [info level 0] 0]]
set ${p}(Name)          $p
set ${p}(type)          {testcase}
set ${p}(Description)   {Performance test with zbridge cfg and QOS and softbypass enabled}
set ${p}(Suite)         {performance_extra}
set ${p}(Comments)      {}
set ${p}(Author)        {wsultani}
set ${p}(Required)      {dut smb {Slot0(port)}}
set ${p}(Created)       {10-01-04}
set ${p}(EOL)           {}
update_mysql_db ${p}
#--------------------------------------------------------------------------------------------#

    package require performance

    ###########################################################
    # User defined values
    ###########################################################

    array set cfg ""
    
    set cfg(cfglst) {zbridgeapp}
    set cfg(ftest) "1"
    set cfg(qos) on
    set cfg(sbypass) on
    set cfg(jumbo) off
    set cfg(cpu) "All"
    set cfg(testtime) 60
    set cfg(framesize) {64 128 200 256 350 384 440 448 500 512 1024 1280 1500 1518}

    ###########################################################
    # End of user defined values
    ###########################################################

    lreturn [perf_common_body cfg]
}

# this is required for auto loading
package provide PER_ZBRIDGE_QOS_SOFTBYPASS 1.0
