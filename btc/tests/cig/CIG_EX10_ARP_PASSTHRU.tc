#!/usr/bin/expect --

proc CIG_EX10_ARP_PASSTHRU {} {
#--------------------------------------------------------------------------------------------#
variable p [namespace tail [lindex [info level 0] 0]]
set ${p}(Name)		$p
set ${p}(type)		{testcase}
set ${p}(Description)	{Test CIG pass-through action using ARP.}
set ${p}(Suite)		{cig}
set ${p}(Comments)	{}
set ${p}(Author)	{jenriq}
set ${p}(Required)	{dut pc0}
set ${p}(Created)	{09-11-12}
set ${p}(EOL)		{}
update_mysql_db ${p}
#--------------------------------------------------------------------------------------------#

    package require cigprocs

    set cfgarr(cfg) zbridgeapp
    set cfgarr(rpmlst) ""
    set cfgarr(mbps) 200
    set cfgarr(pkt_count) 1000
    set cfgarr(pcaplst) {"ether_68B_ARP_1burst1000.pcap"}

    # define testcase xml 
    set cfgarr(testcase_xml) "" 
    set cfgarr(mode) inline

    # define array for comparison vs. cpu stats
    # cpu stats ideals are not defined here, "0" as reference is assumed 

    # define array for comparison vs. xlr stats
    set ingress s0.e0
    set egress s0.e1
    set ideal($ingress\_rx) $cfgarr(pkt_count) 
    set ideal($egress\_tx) $cfgarr(pkt_count) 
    set ideal(stack_rx) [expr $ideal($ingress\_rx) + $ideal($egress\_tx)]

    cigreturn [cig_shared_code]
}

# this is required for auto loading
package provide CIG_EX10_ARP_PASSTHRU 1.0
